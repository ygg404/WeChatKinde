/* view/Tabs/Success/success.wxss */
@import "/view/template/item.wxss";

.success-fl{
  margin-top: 20px;
  width: 96%;
  margin-left: 2%;
}
.flag{
    background-color: #fff;
    border: solid 1px #ededed;
    width: 32.4%;
    text-align: center;
    font-size: 16px;
    color: #000;
    line-height: 38px;
    margin-bottom: 10px;
}
.tr{
  display:flex;
  flex-direction:row;
  align-items: center;

}
.flag1 {
    background-color: #0657a9;
    border: solid 1px #ededed;
    outline: none;
    width: 32.4%;
    text-align: center;
    font-size: 16px;
    color: #fff;
    line-height: 38px;
    margin-bottom: 10px;
}
.alshow{
  display: block;
}
.alhide{
  display: none;
}

.projectshow{
  flex-direction:row;
  align-items:center;
  width:100%;
  display:flex;
  flex-wrap: wrap;
}

.lishow{
  text-align:center;
  margin-top:15rpx;
  width:47%;
  border:1px solid #dedbdb;
  margin-left:1%;
  margin-right:1%;
  height: 170rpx;

}

.lishow image{
  width: 70%;
  height: 70%;
  vertical-align:  middle;
  margin-top: 7%;
}